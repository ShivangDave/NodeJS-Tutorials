<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <link rel="stylesheet" href="/assets/styles.css">
    <meta charset="utf-8">
    <title>Profile</title>
  </head>
  <body>
    <% include partials/nav.ejs %>
    <h1>Welcome, <%= name %> </h1>
    <h2>Job: <%= data.job %> </h2>
    <h2>Age: <%= data.age %> </h2>
    <ul>
      <% data.hobbies.forEach(function(item){ %>
        <li> <%= item %> </li>
      <% }); %>
    </ul>
  </body>
</html>
